<faces-config version="3.0"
              xmlns="https://jakarta.ee/xml/ns/jakartaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-facesconfig_3_0.xsd">

    <!--general-->
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>categorias</from-outcome>
            <to-view-id>/admin/categorias/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>tienda</from-outcome>
            <to-view-id>/admin/tienda/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>categorias</from-outcome>
            <to-view-id>/admin/categorias/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>promocion</from-outcome>
            <to-view-id>/admin/promocion/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>categorias_public</from-outcome>
            <to-view-id>/public/categorias/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>promocion_public</from-outcome>
            <to-view-id>/public/promocion/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>tienda_public</from-outcome>
            <to-view-id>/public/tienda/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <!-- categorias -->
    <navigation-rule>
        <from-view-id>/admin/categorias/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/admin/categorias/edit.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>remove</from-outcome>
            <to-view-id>/admin/categorias/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>create</from-outcome>
            <to-view-id>/admin/categorias/create.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/admin/categorias/edit.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>sucess</from-outcome>
            <to-view-id>/admin/categorias/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>failure</from-outcome>
            <to-view-id>/admin/categorias/edit.xhtml</to-view-id>
        </navigation-case>
       
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/admin/categorias/create.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>sucess</from-outcome>
            <to-view-id>/admin/categorias/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>failure</from-outcome>
            <to-view-id>/admin/categorias/create.xhtml</to-view-id>
        </navigation-case>
       
    </navigation-rule>
    
    
   
   
   
  
   
   
    <!--promocion-->
    <navigation-rule>
        <from-view-id>/admin/promocion/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/admin/promocion/edit.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>remove</from-outcome>
            <to-view-id>/admin/promocion/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>create</from-outcome>
            <to-view-id>/admin/promocion/create.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/admin/promocion/edit.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>sucess</from-outcome>
            <to-view-id>/admin/promocion/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>failure</from-outcome>
            <to-view-id>/admin/promocion/edit.xhtml</to-view-id>
        </navigation-case>
       
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/admin/promocion/create.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>sucess</from-outcome>
            <to-view-id>/admin/promocion/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>failure</from-outcome>
            <to-view-id>/admin/promocion/edit.xhtml</to-view-id>
        </navigation-case>
       
    </navigation-rule>
   
   
   
   
   
   
   
   
   
    <!--tienda-->
    <navigation-rule>
        <from-view-id>/admin/tienda/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/admin/tienda/edit.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>remove</from-outcome>
            <to-view-id>/admin/tienda/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>create</from-outcome>
            <to-view-id>/admin/tienda/create.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/admin/tienda/edit.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>sucess</from-outcome>
            <to-view-id>/admin/tienda/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>remove</from-outcome>
            <to-view-id>/admin/tienda/edit.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>reload</from-outcome>
            <to-view-id>/admin/tienda/edit.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/admin/localidades/edit.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>create</from-outcome>
            <to-view-id>/admin/localidades/create.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>failure</from-outcome>
            <to-view-id>/admin/propiedades/edit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/admin/tienda/create.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>sucess</from-outcome>
            <to-view-id>/admin/tienda/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>failure</from-outcome>
            <to-view-id>/admin/tienda/create.xhtml</to-view-id>
        </navigation-case>
       
    </navigation-rule>
   
</faces-config>